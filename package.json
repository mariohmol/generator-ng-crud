{
  "name": "generator-ng-crud",
  "version": "0.0.0",
  "description": "Angular CRUD generator from JSONSchema",
  "homepage": "https://github.com/mariohmol/generator-ng-crud",
  "author": {
    "name": "Mario Mol",
    "email": "mariohmol@gmail.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "angular crud jsonschema",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "chalk": "^2.1.0",
    "clone": "^2.1.2",
    "glob-promise": "^3.4.0",
    "gulp-rename": "1.2.2",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^7.1.3",
    "json-schema-to-mongoose": "^0.2.2",
    "mongoose": "^5.8.11",
    "natives": "^1.1.6",
    "pascalcase": "^1.0.0",
    "pluralize": "^8.0.0",
    "traverse": "^0.6.6",
    "tv4": "^1.3.0",
    "util": "^0.12.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "mariohmol/generator-ng-crud",
  "license": "Apache-2.0"
}
